{
  "name": "bartoc-search",
  "version": "0.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --config nodemon.json",
    "build:client": "vite build",
    "build:ssr": "vite build --ssr src/client/entry-server.js --outDir dist/server",
    "build": "npm run build:client && npm run build:ssr",
    "start:prod": "cross-env NODE_ENV=production DISABLE_WORKERS=1 tsx ./src/server/start-prod.ts",
    "start:prod:local": "cross-env NODE_ENV=production DISABLE_WORKERS=1 SOLR_HOST=localhost SOLR_PORT=8983 tsx ./src/server/start-prod.ts",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:watch": "vitest",
    "lint": "npm run fix && npm run lint:server",
    "fix": "eslint --fix",
    "lint:server": "tsc --noEmit --project tsconfig.json && eslint --fix \"src/server/**/*.{js,ts}\" --config src/server/eslint.config.mjs",
    "live:seed": "npx ts-node ./src/data/seedTerminologies.ts",
    "update-data": "tsx ./src/server/utils/updateFromBartoc.ts",
    "reindex": "tsx ./src/server/solr/index-once.ts",
    "prepare": "husky || true",
    "yesno": "node ./scripts/yesno.mjs",
    "release": "bash ./scripts/release.sh",
    "release:patch": "SEMVER=patch npm run release",
    "release:minor": "SEMVER=minor npm run release",
    "release:major": "SEMVER=major npm run release"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@bull-board/api": "^6.10.1",
    "@bull-board/express": "^6.10.1",
    "@eslint/js": "^9.24.0",
    "@faker-js/faker": "^9.7.0",
    "@testcontainers/mongodb": "^11.7.1",
    "@tsconfig/node20": "^20.1.5",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/express-ws": "^3.0.5",
    "@types/lodash": "^4.17.16",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.18.12",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "axios": "^1.9.0",
    "c8": "^10.1.3",
    "cocoda-sdk": "^3.4.13",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "eslint": "^9.24.0",
    "eslint-config-gbv": "^2.6.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-vue": "^10.3.0",
    "express-ws": "^5.0.2",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jskos-cli": "^0.8.3",
    "jskos-tools": "^1.0.43",
    "lodash": "^4.17.21",
    "namespace-lookup": "^0.2.1",
    "nodemon": "^3.1.10",
    "portfinder": "^1.0.37",
    "qs": "^6.14.0",
    "supertest": "^7.1.4",
    "testcontainers": "^11.7.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.29.1",
    "vite": "^6.1.1",
    "vitest": "^3.2.4",
    "vue-router": "^4.5.1",
    "ws": "^8.18.2",
    "zod": "^3.24.4"
  },
  "overrides": {
    "eslint": "^9.24.0"
  },
  "dependencies": {
    "bullmq": "^5.53.2",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "feather-icons": "^4.29.2",
    "jskos-vue": "^0.3.18",
    "morgan": "^1.10.0",
    "namespace-lookup": "^0.2.1",
    "sirv": "^3.0.1",
    "vue": "^3.5.25",
    "vue-feather": "^2.0.0"
  }
}
